{
    "name": "@alinazar-111/schema-seed",
    "version": "0.1.0",
    "type": "module",
    "description": "CLI for schema-seed",
    "author": "Ali Nazar",
    "license": "MIT",
    "private": false,
    "repository": {
        "type": "git",
        "url": "https://github.com/alinazar-111/schema-seed.git"
    },
    "homepage": "https://github.com/alinazar-111/schema-seed#readme",
    "bugs": {
        "url": "https://github.com/alinazar-111/schema-seed/issues"
    },
    "keywords": [
        "database",
        "seeding",
        "test-data",
        "cli",
        "schema-seed"
    ],
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "exports": {
        ".": {
            "import": "./dist/index.js"
        }
    },
    "bin": {
        "schema-seed": "./dist/bin.js"
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "tsup",
        "clean": "rm -rf dist",
        "dev": "tsup --watch",
        "lint": "eslint .",
        "test": "vitest run --passWithNoTests",
        "typecheck": "tsc --noEmit",
        "prepublishOnly": "pnpm build"
    },
    "dependencies": {
        "@alinazar-111/schema-seed-core": "workspace:*",
        "@alinazar-111/schema-seed-generators": "workspace:*",
        "commander": "^12.1.0",
        "dotenv": "^17.2.3",
        "jiti": "^2.6.1"
    },
    "devDependencies": {
        "@alinazar-111/schema-seed-adapter-mongodb": "workspace:*",
        "@alinazar-111/schema-seed-adapter-mysql": "workspace:*",
        "@alinazar-111/schema-seed-adapter-postgres": "workspace:*",
        "@alinazar-111/schema-seed-adapter-sqlite": "workspace:*",
        "@alinazar-111/schema-seed-adapter-mssql": "workspace:*",
        "@alinazar-111/schema-seed-adapter-oracle": "workspace:*",
        "@types/node": "^20.19.27",
        "tsup": "^8.3.5",
        "typescript": "^5.7.2",
        "vitest": "^2.1.8"
    }
}