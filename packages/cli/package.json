{
  "name": "@schema-seed/cli",
  "version": "0.0.1",
  "type": "module",
  "description": "CLI for schema-seed",
  "author": "Ali Nazar",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AliNazar-111/schema-seed.git"
  },
  "keywords": [
    "database",
    "seeding",
    "test-data",
    "cli",
    "schema-seed"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "bin": {
    "schema-seed": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "test": "vitest run --passWithNoTests",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@schema-seed/core": "workspace:*",
    "@schema-seed/generators": "workspace:*",
    "@schema-seed/adapter-postgres": "workspace:*",
    "@schema-seed/adapter-mysql": "workspace:*",
    "@schema-seed/adapter-sqlite": "workspace:*",
    "@schema-seed/adapter-mongodb": "workspace:*",
    "commander": "^12.1.0",
    "jiti": "^2.6.1"
  },
  "devDependencies": {
    "@types/node": "^20.19.27",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
