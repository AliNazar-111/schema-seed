{
    "name": "@alinazar-111/schema-seed-core",
    "version": "0.1.0",
    "type": "module",
    "description": "Core logic for schema-seed",
    "author": "Ali Nazar",
    "license": "MIT",
    "private": false,
    "sideEffects": false,
    "repository": {
        "type": "git",
        "url": "https://github.com/alinazar-111/schema-seed.git"
    },
    "homepage": "https://github.com/alinazar-111/schema-seed#readme",
    "bugs": {
        "url": "https://github.com/alinazar-111/schema-seed/issues"
    },
    "keywords": [
        "database",
        "seeding",
        "test-data",
        "schema-seed"
    ],
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        },
        "./mongo": {
            "types": "./dist/mongo/types.d.ts",
            "import": "./dist/mongo/types.js",
            "require": "./dist/mongo/types.cjs"
        }
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "tsup",
        "clean": "rm -rf dist",
        "dev": "tsup --watch",
        "lint": "eslint .",
        "test": "vitest run --passWithNoTests",
        "typecheck": "tsc --noEmit",
        "prepublishOnly": "pnpm build"
    },
    "devDependencies": {
        "tsup": "^8.3.5",
        "typescript": "^5.7.2",
        "vitest": "^2.1.8"
    }
}